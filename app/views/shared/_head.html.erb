<head>
  <meta charset="utf-8" >
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="viewport" content="width=device-width" />
  <title><%= yield_content :title %></title>
  <meta name="description" content="<%= yield_content :description %>" />
  <meta name="keywords" content="<%= yield_content :meta_keywords %>" />
  <meta property="fb:app_id" content="<%= yield_content :meta_title %>" />
  <meta name="google-site-verification" content="<%= yield_content t('meta.google_site_verification') %>" />
  <meta property="og:title" content="<%= yield_content :meta_title %>"/>
  <meta property="og:type" content="<%= yield_content :type %>"/>
  <meta property="og:site_name" content="<%= yield_content :fb_site_name %>"/>
  <meta property="og:description" content="<%= yield_content :fb_description %>"/>
  <%= stylesheet_link_tag "//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css", "all" %>
  <%= stylesheet_link_tag "//cloud.typography.com/6902032/784442/css/fonts.css", "all" %>
  <%= javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js" %>
  <%= javascript_include_tag "modernizr", "plugins", "all" %>
  <%= csrf_meta_tags %>
  <%= yield_content :head %>
</head>
